# Konfigurasi Bot Pharos - JANGAN SHARE FILE INI!
PHAROS_RPC_URL=https://pharos-testnet-rpc.com
PHAROS_CHAIN_ID=1234
PHAROS_API_BASE=https://api.pharos.network

# Konfigurasi Keamanan
ENCRYPTION_KEY=your-32-character-encryption-key-here
MAX_RETRY_ATTEMPTS=3
DELAY_BETWEEN_ACCOUNTS=5000
DELAY_BETWEEN_TASKS=2000

# Konfigurasi Proxy (opsional)
USE_PROXY=false
PROXY_ROTATION=true

# Konfigurasi Logging
LOG_LEVEL=info
LOG_TO_FILE=true
